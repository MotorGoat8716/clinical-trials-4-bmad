{
  "name": "clinical-trials-api",
  "version": "1.0.0",
  "description": "API for searching clinical trials.",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "db:seed": "node db/init-db.js && node db/fetch-and-seed.js",
