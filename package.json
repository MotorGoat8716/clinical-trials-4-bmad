    "express": "^4.17.1",
    "pg": "^8.7.1"
  },
  "devDependencies": {
    "dotenv": "^16.6.0",
    "jest": "^27.2.5",
    "supertest": "^6.1.6",
    "concurrently": "^7.6.0"
  }
}
